{
  "name": "@dfuse/client",
  "version": "0.2.0-rc.4",
  "description": "dfuse JavaScript/TypeScript Client Library (for [dfuse API](https://docs.dfuse.io/))",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "dist/*"
  ],
  "repository": "https://github.com/dfuse-io/client-js",
  "author": {
    "name": "dfuse Developers",
    "email": "dev@dfuse.io",
    "url": "https://dfuse.io"
  },
  "contributors": [
    {
      "name": "Denis Carriere",
      "email": "denis@eosnation.io",
      "url": "https://eosnation.io"
    }
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf build dist",
    "docs": "rimraf build && typedoc",
    "prepare": "tsc",
    "lint": "tslint **/*.ts",
    "lint:specific": "tslint",
    "test": "jest",
    "run:example": "yarn run ts-node"
  },
  "husky": {
    "hooks": {
      "_comment": "Use --no-verify to bypass",
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.ts": [
        "yarn lint:specific"
      ]
    },
    "ignore": [
      "**/*.d.ts"
    ]
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^23.3.9",
    "@types/node-fetch": "^2.1.6",
    "@types/ws": "^6.0.1",
    "dotenv": "^6.1.0",
    "husky": "^1.1.2",
    "jest": "^24.1.0",
    "lint-staged": "^7.3.0",
    "node-fetch": "^2.3.0",
    "patch-package": "^6.0.7",
    "postinstall-postinstall": "^2.0.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.14.2",
    "typedoc-default-themes-extension": "^0.0.3",
    "typedoc-plugin-toc-group": "^0.0.4",
    "typescript": "^3.1.3",
    "ws": "^6.1.0"
  },
  "dependencies": {
    "debug": "^4.1.0"
  }
}
